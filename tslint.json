{
  "extends": "tslint:recommended",
  "rules": {
    "max-line-length": {
      "options": [
        80
      ]
    },
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-console": [false],
    "no-var-requires": false,
    "prefer-for-of": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "variable-name": ["allow-leading-underscore"]
  },
  "jsRules": {
    "max-line-length": {
      "options": [
        80
      ]
    }
  }
}