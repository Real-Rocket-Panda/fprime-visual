{
  "name": "fprime-editor",
  "version": "0.2.0",
  "author": "The Jet Trio",
  "description": "FPP language visualizing and editing tool",
  "license": "",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:all": "node .electron-vue/build.js && electron-builder -mwl",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "e2e": "npm run pack && mocha test/e2e",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "test": "npm run fprime-test && npm run ui-test && npm run e2e",
    "test:fprime": "nyc mocha --opts ./test/fprime/mocha.opts",
    "test:ui": "karma start test/ui/karma.conf.js",
    "postinstall": "",
    "tslint": "tslint -c ./tslint.json -p ./tsconfig.json"
  },
  "nyc": {
    "include": [
      "src/fprime/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html",
      "text-summary",
      "lcov"
    ],
    "all": true
  },
  "build": {
    "productName": "fprime-editor",
    "appId": "edu.cmu.isr",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    },
    "asar": false
  },
  "dependencies": {
    "acorn": "^5.6.2",
    "css": "^2.2.4",
    "cytoscape": "^3.8.1",
    "cytoscape-cola": "^2.2.3",
    "cytoscape-context-menus": "^3.0.6",
    "cytoscape-cose-bilkent": "^4.0.0",
    "cytoscape-dagre": "^2.2.1",
    "cytoscape-edge-bend-editing": "^1.6.1",
    "cytoscape-edgehandles": "^3.5.1",
    "cytoscape-klay": "^3.1.1",
    "cytoscape-popper": "^1.0.1",
    "jquery": "^3.3.1",
    "konva": "^1.6.3",
    "lodash": "^4.17.11",
    "rp-automove": "^1.9.0",
    "rp-cytoscape-node-resize": "^3.1.2",
    "sprintf-js": "^1.1.1",
    "tippy.js": "^2.5.3",
    "vue": "^2.6.10",
    "vue-codemirror": "^4.0.6",
    "vue-color": "^2.4.6",
    "vue-electron": "^1.0.6",
    "vue-router": "^3.0.7",
    "vuetify": "^1.1.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/cytoscape": "3.2.1",
    "@types/es6-promise": "^3.3.0",
    "@types/mocha": "^5.2.7",
    "ajv": "^6.10.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^1.1.3",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.4",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^2.0.0",
    "electron-builder": "^20.15.0",
    "electron-debug": "^1.4.0",
    "electron-devtools-installer": "^2.2.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "growl": "^1.10.5",
    "html-webpack-plugin": "^2.30.1",
    "inject-loader": "^3.0.0",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-electron": "^5.1.1",
    "karma-mocha": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-typescript": "^3.0.13",
    "karma-webpack": "^2.0.1",
    "mocha": "^3.0.2",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "nyc": "^12.0.2",
    "require-dir": "^0.3.0",
    "source-map-support": "^0.5.6",
    "spectron": "^3.7.1",
    "style-loader": "^0.18.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "ts-loader": "^3.5.0",
    "ts-node": "^6.1.0",
    "tsconfig-paths": "^3.4.0",
    "tslint": "^5.18.0",
    "typescript": "^2.9.2",
    "url-loader": "^0.5.9",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.7.3",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^3.5.2",
    "webpack-dev-server": "^2.11.5",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.1"
  }
}
